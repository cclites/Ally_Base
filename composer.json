{
    "name": "jtr-solutions/krioscare",
    "description": "KriosCare health care app",
    "keywords": [],
    "license": "",
    "type": "project",
    "version": "1.0.0",
    "time": "2019-11-20",
    "require": {
        "php": ">=7.2.0",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "barryvdh/laravel-snappy": "^0.4.0",
        "bizhub/impersonate": "^0.2.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.0",
        "giggsey/libphonenumber-for-php": "^8.8",
        "guzzlehttp/psr7": "^1.5",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "inacho/php-credit-card-validator": "^1.0",
        "laravel/framework": "^6.0",
        "laravel/tinker": "~1.0",
        "maatwebsite/excel": "^3.1",
        "mikehaertl/php-pdftk": "^0.7.0",
        "myclabs/php-enum": "^1.6",
        "nacha/file-generator": "1.9.*",
        "owen-it/laravel-auditing": "9.*",
        "php-http/curl-client": "^1.7",
        "phpseclib/phpseclib": "^2.0",
        "quickbooks/v3-php-sdk": "^5.0.4",
        "rlanvin/php-rrule": "^1.6",
        "s-ichikawa/laravel-sendgrid-driver": "^2.0",
        "sentry/sentry-laravel": "1.6.*",
        "tplaner/when": "^2.1",
        "twilio/sdk": "^5.42",
        "ua-parser/uap-php": "^3.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.8",
        "laravel/homestead": "^6.1",
        "mockery/mockery": "0.9.*",
        "overtrue/phplint": "^1.1",
        "phpunit/phpunit": "^8.0",
        "wemersonjanuario/wkhtmltopdf-windows": "0.12.2.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "bootstrap/functions.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Packages\\": "packages/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2.10"
        }
    }
}
